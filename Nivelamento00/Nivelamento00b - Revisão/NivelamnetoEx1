#include <stdio.h>
#include <stdlib.h>
#include <stdbool.h>
bool primeiro(int arr[], int x, int tam)
{
  bool contido=false;
  for(int i=0; i<tam; i++)
    {
      if(arr[i]==x)
      {
        contido=true;
      }
    }
  return contido;
}

int main() 
{
  int arr[]={1,2,3,4,5};
  int x=4;
  int tam= sizeof(arr)/sizeof(arr[0]);
  primeiro(arr,x,tam);
  if(primeiro(arr,x,tam))
  {
    printf("\nO número esta contido no array");
  }
  else
  {
    printf("\nO número não esta contido no array");
  }
  return 0;
}
